{
  "name": "lambda",
  "version": "0.1.0",
  "description": "Lambda runtime code for cloud gaming platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "docker:build": "npm run build && docker build -t lunaris-lambda .",
    "docker:run": "docker run --rm -p 9000:8080 lunaris-lambda"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.156",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.9",
    "aws-sdk-client-mock": "^4.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.917.0",
    "@aws-sdk/client-ec2": "^3.917.0",
    "@aws-sdk/client-ssm": "^3.917.0",
    "@aws-sdk/client-sfn": "^3.917.0",
    "@aws-sdk/lib-dynamodb": "^3.917.0"
  }
}